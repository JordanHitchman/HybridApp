        <activity
            android:name=".login"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".register"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".dashboard"
            android:label="@string/title_activity_dashboard" >
        </activity>
        <activity
            android:name=".fp"
            android:label="@string/title_activity_fp" >
        </activity>
        <activity
            android:name=".TakePicture"
            android:label="@string/title_activity_take_picture" >
        </activity>
        <activity
            android:name=".fitbit"
            android:label="@string/title_activity_fitbit" >
        </activity>
        <activity
            android:name=".fitbitWeb"
            android:label="@string/title_activity_fitbit_web" >
        </activity>
    </application>

</manifest>
